	<templates>
		<template name="page_n">
			<background color="%background_color2%"/>
			
			<fill color="#00000030">
				<condition var1="tw_simulate_actions" var2="0"/>
				<placement x="0" y="0" w="%screen_width%" h="%status_height%"/>
			</fill>
			
			<fill color="%accent%">
				<placement x="0" y="0" w="1080" h="240"/>
			</fill>
			
			<fill color="%middle%">
				<placement x="3" y="258" w="1074" h="1513"/>
			</fill>
			
			<fill color="%text_fail_color%">
				<condition var1="tw_simulate_actions" var2="1"/>
				<placement x="0" y="0" w="%screen_width%" h="%status_height%"/>
			</fill>

			<text color="%header_color%">
				<font resource="font_m"/>
				<placement x="15" y="%row1_header_y%"/>
				<text>%tw_time%</text>
			</text>
			
			<text color="%header_color%">
				<condition var1="show_theme_version" var2="0"/>
				<font resource="font_m"/>
				<placement x="15" y="190"/>
				<text>%theme_version%</text>
			</text>
			
			<text color="%header_color%">
				<condition var1="show_theme_version" var2="1"/>
				<font resource="font_m"/>
				<placement x="15" y="190"/>
				<text>SD %tw_storage_free_size% Mb</text>
			</text>

			<text color="%header_color%">
				<conditions>
					<condition var1="tw_no_battery_percent" var2="0"/>
					<condition var1="tw_battery" op="&gt;" var2="0"/>
					<condition var1="tw_battery" op="&lt;" var2="101"/>
				</conditions>
				<font resource="font_m"/>
				<placement x="990" y="190"/>
				<text>%tw_battery%</text>
			</text>
			
			<text color="%header_color%">
				<condition var1="show_cpu_temp" var2="0"/>
				<font resource="font_m"/>
				<placement x="960" y="%row1_header_y%"/>
				<text>%tw_version%</text>
			</text>

			<text color="%header_color%">
				<condition var1="show_cpu_temp" var2="1"/>
				<font resource="font_m"/>
				<placement x="890" y="%row1_header_y%"/>
				<text>{@cpu_temp=CPU: %tw_cpu_temp% °C}</text>
			</text>
			
			<fill color="%middle%">
				<placement x="0" y="%navbar_y%" w="%screen_width%" h="%navbar_height%"/>
			</fill>

			<button>
				<condition var1="show_extra_buttons" var2="1"/>
				<placement x="%reload_button_x%" y="%navbar_btn_y%" placement="4"/>
				<image resource="reload"/>
				<condition var1="tw_busy" var2="0"/>
				<action function="reload"/>
			</button>
			
			<button>
				<condition var1="show_extra_buttons" var2="1"/>
				<placement x="%reboot_button_x%" y="%navbar_btn_y%" placement="4"/>
				<image resource="reboot"/>
				<condition var1="tw_busy" var2="0"/>
				<action function="page">reboot_system_routine</action>
			</button>

			<button>
				<condition var1="tw_samsung_navbar" op="!=" var2="1"/>
				<placement x="%back_button_x%" y="%navbar_btn_y%" placement="4"/>
				<image resource="back" highlightresource="back_hl"/>
				<condition var1="tw_busy" var2="0"/>
				<action function="key">back</action>
